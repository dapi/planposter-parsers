{
    "context" : "//div[@class='columns'][1]",
    "data" :
    {
        "title"         : "string(.//h1)",
        "poster"        : "string(.//div[contains(@class,'coursePoster')]//img/@src)",
        "dateStart"     : "string(.//div[@class='date']/span[@class='start'])",
        "dateEnd"       : "string(.//div[@class='date']/span[@class='finish'])",
        "duration"      : "string(.//div[@class='duration']/strong)",
        "timeString"    : "string(.//div[@class='timetable'])",
        "short"         : "string(.//div[starts-with(@class,'courseDescription')]/p)",
        "form"          : "string(.//div[starts-with(@class,'formDescription')]/p)",
        "requirements"  : "string(.//li[starts-with(@class,'requirements')]/p[2])",
        "price"         : "string(.//div[@class='price']/strong)",
        "priceExt"      :
        {
            "context"   : "//div[contains(@class,'prices')]/ul/li",
            "data"  :
            [{
                "price" : "string(p)"
            }]
        },
        "place" :
        {
            "context"   : ".//div[@class='sidebar']//div[contains(@class,'place')]",
            "data"  :
            {
                "phone"     : "string(p[1])",
                "site"      : "string(p[2]/a[1])",
                "email"     : "string(p[2]/a[2])",
                "title"     : "string(p[@class='placeName']/a)",
                "link"      : "string(p[3]/a/@href)",
                "address"   : "string(p[4]/a)",
                "addressUrl": "string(p[4]/a/@href)"
            }
        },
        "teachers" :
        {
            "context"   : "//div[@class='presentersAndOrganizer']/ul/li[1]",
            "data"      :
            [{
                "title" : "string(h2)",
                "photo" : "string(a/img/@src)",
                "url"   : "string(h2/a/@href)",
                "short" : "string(.//div[@class='body']/p)"
            }]
        },
        "organizers" :
        {
            "context"   : "//div[@class='presentersAndOrganizer']/ul/li[2]", 
            "data"      : 
            [{
                "title" : "string(h2)",
                "photo" : "string(a/img/@src)",
                "url"   : "string(h2/a/@href)",
                "short" : "string(.//div[@class='body']/p)"
            }]
        }
    }
}
